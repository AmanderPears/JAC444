<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.ClientApp">
   <children>
      <VBox prefHeight="368.0" prefWidth="335.0">
         <children>
            <TextArea fx:id="displayFld" editable="false" prefHeight="329.0" prefWidth="335.0" wrapText="true" />
            <Button fx:id="sendBtn" mnemonicParsing="false" onAction="#sendMsg" prefHeight="37.0" prefWidth="333.0" text="Send">
               <graphic>
                  <TextField fx:id="sendFld" prefHeight="27.0" prefWidth="278.0" />
               </graphic>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
