<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="193.0" prefWidth="649.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="task2.Address">
	<children>
		<FlowPane prefHeight="176.0" prefWidth="649.0">
			<children>
				<Label text="First Name">
					<padding>
						<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
					</padding>
				</Label>
				<TextField fx:id="firstNameField" prefHeight="27.0" prefWidth="530.0" />
				<Label text="Last Name">
					<padding>
						<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
					</padding>
				</Label>
				<TextField fx:id="lastNameField" prefHeight="27.0" prefWidth="530.0" />
				<Label text="City">
					<padding>
						<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
					</padding>
				</Label>
				<TextField fx:id="cityField" prefWidth="150.0" />
				<Label text="Province">
					<padding>
						<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
					</padding>
				</Label>
				<ChoiceBox fx:id="provinceList" prefWidth="130.0">
					<FlowPane.margin>
						<Insets />
					</FlowPane.margin>
				</ChoiceBox>
				<Label text="Postal Code">
					<padding>
						<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
					</padding>
				</Label>
				<TextField fx:id="postalField" prefWidth="120.0" />
				<Button fx:id="addBtn" mnemonicParsing="false" onAction="#addAddress" prefWidth="80.0" text="Add">
					<FlowPane.margin>
						<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
					</FlowPane.margin>
				</Button>
				<Button fx:id="firstBtn" mnemonicParsing="false" onAction="#firstAdd" prefWidth="80.0" text="First">
					<FlowPane.margin>
						<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
					</FlowPane.margin>
				</Button>
				<Button fx:id="nextBtn" mnemonicParsing="false" onAction="#nextAdd" prefWidth="80.0" text="Next">
					<FlowPane.margin>
						<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
					</FlowPane.margin>
				</Button>
				<Button fx:id="prevBtn" mnemonicParsing="false" onAction="#prevAdd" prefWidth="80.0" text="Previous">
					<FlowPane.margin>
						<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
					</FlowPane.margin>
				</Button>
				<Button fx:id="lastBtn" mnemonicParsing="false" onAction="#lastAdd" prefWidth="80.0" text="Last">
					<FlowPane.margin>
						<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
					</FlowPane.margin>
				</Button>
				<Button fx:id="updateBtn" mnemonicParsing="false" onAction="#updateAdd" prefWidth="80.0" text="Update">
					<FlowPane.margin>
						<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
					</FlowPane.margin>
				</Button>
            <Label fx:id="statusLabel" contentDisplay="RIGHT" prefHeight="17.0" prefWidth="600.0" text="..." textAlignment="RIGHT">
               <padding>
                  <Insets left="10.0" right="10.0" />
               </padding>
            </Label>
			</children>
			<padding>
				<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
			</padding>
		</FlowPane>
		
	</children>
</AnchorPane>
