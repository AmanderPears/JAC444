<AnchorPane maxHeight="358.0" maxWidth="252.0" minHeight="358.0" minWidth="252.0" prefHeight="358.0" prefWidth="252.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="task1.BalanceController">
